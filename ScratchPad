desktopvirtualizationresources
| extend SessionHostPoolName = tolower(tostring(split(name, "/")[0]))
| extend SessionHostName = tolower(tostring(split(name, "/")[1]))
| extend ComputerName = tostring(split(SessionHostName, ".")[0])
| project SessionHostPoolName, SessionHostName, ComputerName, ['id'], subscriptionId



Resources
| where type == "microsoft.desktopvirtualization/sessionhosts"
| extend SessionHostPoolName = tolower(tostring(split(name, "/")[0]))
| extend SessionHostName = tolower(tostring(split(name, "/")[1]))
| project SessionHostPoolName, SessionHostName, name, type

| extend NormalizedResourceGroup = tolower(ResourceGroup)  // Convert ResourceGroup to lowercase

# Import the group names from the file
$groupNames = Get-Content "C:\Path\To\file.txt"  # Replace with actual path if not running inline

# Prepare an empty list to store results
$results = @()

foreach ($group in $groupNames) {
    Write-Output "Processing group: $group"
    try {
        $members = Get-ADGroupMember -Identity $group -ErrorAction Stop
        foreach ($member in $members) {
            $results += [PSCustomObject]@{
                GroupName       = $group
                MemberName      = $member.Name
                SamAccountName  = $member.SamAccountName
                ObjectClass     = $member.objectClass
            }
        }
    }
    catch {
        Write-Warning "Could not retrieve members of group: $group. Error: $_"
    }
}

# Export results to CSV
$results | Export-Csv -Path "$env:USERPROFILE\Desktop\GroupMembers.csv" -NoTypeInformation


<# 
This script will update Microsoft 365 apps on an Image VM without turning on automatic updates.
It downloads automatically downloads the latest version of ODT and uses it to update M35 Apps.

Please edit $ODTConfig if you use a non-standard deployment of M365 (i.e. leaving out powerpoint)
$ODTConfig is what ODT will use for configuration. 
see https://docs.microsoft.com/en-us/deployoffice/overview-office-deployment-tool 
and https://docs.microsoft.com/en-us/deployoffice/office-deployment-tool-configuration-options
for details on ODT configuration XML documentation.

To learn more about the process this script does, 
See https://docs.microsoft.com/en-us/azure/virtual-desktop/install-office-on-wvd-master-image 
#>

# create directory on C: Drive (Temp drive for Azure VM, is removed regularly)
mkdir C:\temp\odt\raw

# parse through the MS Download Center page to get the most up-to-date download link
$MSDlSite2 = Invoke-WebRequest "https://www.microsoft.com/en-us/download/details.aspx?id=49117" -UseBasicParsing
ForEach ($Href in $MSDlSite2.Links.Href)
{
    if ($Href -match "officedeploymenttool" ){
        $DLink = $href
    }
}

# Download office deployment tool using up-to-date link grabed eariler
Invoke-WebRequest -Uri $DLink -OutFile "C:\temp\odt.exe" -UseBasicParsing

# unpack the ODT executable to get the setup.exe
$app = 'C:\temp\odt.exe'
$arg1 = '/quiet'
$arg2 = '/extract:C:\temp\odt\raw'
& $app $arg1 $arg2

# create a base config XML for ODT to use, this one has auto-update disabled
$ODTConfig = @"
<Configuration>
  <Add OfficeClientEdition="64" Channel="MonthlyEnterprise">
    <Product ID="O365ProPlusRetail">
      <Language ID="en-gb" />
      <Language ID="MatchOS" />
      <ExcludeApp ID="Groove" />
      <ExcludeApp ID="Lync" />
      <ExcludeApp ID="OneDrive" />
      <ExcludeApp ID="Teams" />
      <ExcludeApp ID="Publisher" />
    </Product>
    <Product ID="VisioProRetail">
      <Language ID="en-gb" />
      <Language ID="MatchOS" />
      <ExcludeApp ID="Groove" />
      <ExcludeApp ID="Lync" />
      <ExcludeApp ID="OneDrive" />
      <ExcludeApp ID="Teams" />
      <ExcludeApp ID="Publisher" />
    </Product>
    <Product ID="ProjectProRetail">
      <Language ID="en-gb" />
      <Language ID="MatchOS" />
      <ExcludeApp ID="Groove" />
      <ExcludeApp ID="Lync" />
      <ExcludeApp ID="OneDrive" />
      <ExcludeApp ID="Teams" />
      <ExcludeApp ID="Publisher" />
    </Product>
  </Add>
  <RemoveMSI/>
  <Updates Enabled="FALSE"/>
  <Display Level="None" AcceptEULA="TRUE" />
  <Logging Level="Standard" Path="%temp%\WVDOfficeInstall" />
  <Property Name="FORCEAPPSHUTDOWN" Value="TRUE"/>
  <Property Name="SharedComputerLicensing" Value="1"/>
</Configuration>
"@ 
$ODTConfig | Out-File "C:\temp\odt\raw\odtconfig.xml"

# execute odt.exe using the newly created odtconfig.xml. This updates/installs office (takes a while)
$app2 = 'C:\temp\odt\raw\setup.exe'
$arg21 = '/configure'
$arg22 = 'C:\temp\odtconfig.xml'
& $app2 $arg21 $arg22





