    WVDConnections
    | where State == "Connected"
    | project EndTime = TimeGenerated, CorrelationId
) on CorrelationId
| project CorrelationId, UserName, Duration = EndTime - StartTime, ConnectionType, StartTime, EndTime, _ResourceId
