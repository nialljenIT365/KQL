$url = "https://management.azure.com$vmResourceId/instanceView?api-version=2021-03-01"

# Define API request parameters
$parameters = @{
    URI     = $url 
    Method  = "GET"
    Headers = $token
}

# Make the API request
$response = Invoke-RestMethod @parameters

# Display the VM status information
$response.statuses






