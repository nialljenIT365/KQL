WVDCheckpoints
| where Source == "RDBroker"
| where Name == "LoadBalancedNewConnection"
| extend ParametersString = tostring(Parameters)
| distinct ParametersString




extract(@"[rR]esource[Gg]roups/([^/]+)", 1, _ResourceId)

| extend 
    ResourceGroup = extract(@"resourceGroups/([^/]+)", 1, _ResourceId),
    HostPoolName = extract(@"hostpools/([^/]+)", 1, _ResourceId)
| distinct ResourceGroup, HostPoolName



Perf
| where ObjectName == "User Input Delay per Process"
| where InstanceName contains "rdpvchost.exe"
| summarize LatestInstanceName = arg_max(TimeGenerated, InstanceName) by bin(TimeGenerated, 30m)



Perf
| where ObjectName == "User Input Delay per Process"
| where InstanceName contains "rdpvchost.exe"
| extend TimeBin = bin(TimeGenerated, 30m)  // Group into 30-minute bins
| summarize FirstEntry = min(TimeGenerated) by TimeBin, InstanceName, Computer
| order by TimeBin asc, FirstEntry asc



let HostPoolMapping = datatable(SessionHostKey: string, HostPoolName: string)
[
