Heartbeat
| extend RangeStart = "5/11/2024 11:00:00"
| extend Day = substring(RangeStart, 0, indexof(RangeStart, "/"))
| extend Month = substring(RangeStart, indexof(RangeStart, "/") + 1, indexof(RangeStart, "/", indexof(RangeStart, "/") + 1) - indexof(RangeStart, "/") - 1)
| extend Year = substring(RangeStart, indexof(RangeStart, "/", indexof(RangeStart, "/") + 1) + 1, 4)
| extend Time = substring(RangeStart, indexof(RangeStart, " ") + 1)
| extend fRangeStart = strcat(Year, "-", Month, "-", Day, "T", Time)
| project RangeStart, fRangeStart
