Heartbeat
| extend OriginalDateString = "5/11/2024 11:00:00"
| extend Day = substring(OriginalDateString, 0, indexof(OriginalDateString, "/"))
| extend Month = substring(OriginalDateString, indexof(OriginalDateString, "/") + 1, indexof(OriginalDateString, "/", indexof(OriginalDateString, "/") + 1) - indexof(OriginalDateString, "/") - 1)
| extend Year = substring(OriginalDateString, indexof(OriginalDateString, "/", indexof(OriginalDateString, "/") + 1) + 1, 4)
| extend Time = substring(OriginalDateString, indexof(OriginalDateString, " ") + 1)
| extend FormattedDateString = strcat(Year, "-", Month, "-", Day, "T", Time)
