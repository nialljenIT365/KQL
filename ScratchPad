let
    Source = #"11-AVD ALL Errors (WVDErrors)",
    RemovedColumns = Table.RemoveColumns(
        Source,
        {
            "TenantId", 
            "TimeGenerated", 
            "UserName", 
            "ActivityType", 
            "Source", 
            "CodeSymbolic", 
            "Message", 
            "ServiceError", 
            "Operation", 
            "SourceSystem", 
            "Type", 
            "_ResourceId", 
            "Date", 
            "Time", 
            "TimeKey"
        }
    ),
    GroupedRows = Table.Group(
        RemovedColumns, 
        {"CorrelationId"}, 
        {{"Codes", each Text.Combine(List.Sort([Code]), ", ")}}
    )
in
    GroupedRows
